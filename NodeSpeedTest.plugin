#!name = 节点测速
#!desc = 定时自动测试节点延迟和速度，支持手动运行。每6小时自动测试一次，可在插件页面手动执行测试
#!author = Claude
#!icon = https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Speedtest.png
#!homepage = https://github.com/rdone4425/yuming-ip
#!date = 2024-03-21

[Script]
# 手动运行脚本
generic script-path=https://raw.githubusercontent.com/rdone4425/yuming-ip/refs/heads/main/NodeSpeedTest.js, tag=节点测速, img-url=https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Speedtest.png, timeout=300

# 定时运行脚本
cron "0 */6 * * *" script-path=https://raw.githubusercontent.com/rdone4425/yuming-ip/refs/heads/main/NodeSpeedTest.js, tag=定时测速, img-url=https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Speedtest.png, timeout=300

[MITM]
hostname = www.gstatic.com, cachefly.cachefly.net

[URL Rewrite]
^https?:\/\/(www.)?(g|google)matic.com\/generate_204 - reject

[Panel]
NodeSpeedTest = script-name=节点测速, title=节点测速, content=点击执行测速, update-interval=600 
